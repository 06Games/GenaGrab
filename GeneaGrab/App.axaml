<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:helpers="clr-namespace:GeneaGrab.Helpers"
             xmlns:styling="clr-namespace:FluentAvalonia.Styling;assembly=FluentAvalonia"
             x:Class="GeneaGrab.App">
    
    <Application.Resources>
        <helpers:BitmapAssetValueConverter x:Key="VariableImage"/>
        <helpers:JoinConvertor x:Key="JoinConverter" />
        <helpers:ResourceConverter x:Key="Translate"/>
    </Application.Resources>

    <Application.Styles>
        <styling:FluentAvaloniaTheme />
    </Application.Styles>
</Application>
